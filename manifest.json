{
    "name": "Extension convert slack message",
    "version": "1.0",
    "description": "Uses OAuth to connect to Google's Spreadsheet API",
    "icons": {
      "128": "images/icon128.png"
    },
    "manifest_version": 2,
    "browser_action": {
      "default_title": "Extension convert slack message",
      "default_popup": "popup.html"
    },
    "key": "",
    "oauth2": {
        "client_id": "838030992109-ashns9in5470hs4a95ci6r9jkqs2jess.apps.googleusercontent.com",
        "scopes":["https://www.googleapis.com/auth/spreadsheets"]
    },
    "permissions": [
        "identity"
    ],
    "content_security_policy": "script-src 'self' https://apis.google.com/; object-src 'self'"
  }